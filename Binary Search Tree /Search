#include<iostream>
using namespace std;

class node{
    public:
    int key;
    node* left;
    node* right;
};

node* search(node* root, int x){
    if(root == NULL || root -> key == x){
        return root;
    }

    if(root -> key < x)
        return search(root -> right, x);
    if(root -> key > x)
        return search(root -> left, x);
}
